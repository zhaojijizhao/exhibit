          <td></td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
        <%if(detail.sth.length>0){
          for(var i = 0; i < detail.sth.length; i++){
          var v=detail.sth[i];%>
          <tr data-id="<%=v._id%>" data-type="sth">
            <td></td>
            <td><%=v.name%></td>
            <td class="tr"><%=v.price%></td>
            <td class="tr"></td>
            <td class="tr"><%=v.people%></td>
            <td class="tr"><%=v.total%></td>
          </tr>
        <%}}%>
        <tr class="b">
          <td></td>
          <td class="tr">小计</td>
          <td colspan="4" class="tr sky"><%=detail.sth_all%></td>
        </tr>
        <tr class="b">
          <td class="tc">7</td>
          <td>陪同费用</td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
          <tr class="">
            <td></td>
            <td>陪同人员</td>
            <td class="tr"><%=detail.with.with_price%></td>
            <td class="tr"><%=detail.with.with_people%></td>
            <td class="tr"><%=detail.with.with_people_num%></td>
            <td class="tr"><%=detail.with.with_total%></td>
          </tr>
          <tr class="">
            <td></td>
            <td>地接人员</td>
            <td class="tr"><%=detail.with.catch_price%></td>
            <td class="tr"><%=detail.with.catch_people%></td>
            <td class="tr"><%=detail.with.catch_people_num%></td>
            <td class="tr"><%=detail.with.catch_total%></td>
          </tr>
        <% var with_all = 0;
          if(detail.with.with_total){
            with_all += detail.with.with_total;
          }
          if(detail.with.catch_total){
            with_all += detail.with.catch_total;
          }%>
        <tr class="b">
          <td></td>
          <td class="tr">小计</td>
          <td colspan="4" class="tr sky"><%=with_all%></td>
        </tr>
        <tr class="b">
          <td class="tc">8</td>
          <td>服务费:按照总活动金额%计算</td>
          <td></td>
          <td></td>
          <td></td>
          <td></td>
        </tr>
        <%var feeptype = ["不收","6%","12%"];%>
        <tr data-id="<%=detail.fee._id%>" data-type="fee">
          <td></td>
          <td></td>
          <td class="tr"><%=detail.total-detail.fee.total%></td>
          <td></td>
          <td class="tr"><%=feeptype[detail.fee.percent]%></td>
          <td class="tr"><%=detail.fee.total%></td>
        </tr>
        <tr class="b">
          <td></td>
          <td class="tr">费用总计：</td>
          <td colspan="4" class="tr sky"><%=detail.total%></td>
        </tr>
      </table>
    </div>
  </div>
</div>
<script src="/lib/jquery/dist/jquery.min.js"></script>
<script src="/lib/html2canvas/build/html2canvas.min.js"></script>
<script src="/lib/canvas2image/canvas2image/canvas2image.js"></script>
<script>
  var pdfimg ="";
  var pdfcallback = function (){};
  var width, height;
  function saveHtml2Image(objId, scale) {
      var obj = $("#" + objId);
      width = obj.width()*1.5;
      height = obj.height()*1.5;
      html2canvas(obj, {
        width: width,
        height: height,
        background: "#fff",
        onrendered: function (canvas) {
          pdfimg = canvas.toDataURL();
          pdfcallback();
       }
      });
    }
    saveHtml2Image('main',1);
</script>
<script src="/lib/jspdf/dist/jspdf.min.js"></script>
<script >
  pdfcallback = function(){
    var doc = new jsPDF();
    doc.addImage(pdfimg, 'PNG', 15, 15, 180, 180/width*height);
    doc.save('aa.pdf');
  }
</script>


